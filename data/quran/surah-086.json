{"surahNumber":86,"ayahs":[{"number":1,"text":"وَٱلسَّمَآءِ وَٱلطَّارِقِ"},{"number":2,"text":"وَمَآ أَدۡرَىٰكَ مَا ٱلطَّارِقُ"},{"number":3,"text":"ٱلنَّجۡمُ ٱلثَّاقِبُ"},{"number":4,"text":"إِن كُلُّ نَفۡسٖ لَّمَّا عَلَيۡهَا حَافِظٞ"},{"number":5,"text":"فَلۡيَنظُرِ ٱلۡإِنسَٰنُ مِمَّ خُلِقَ"},{"number":6,"text":"خُلِقَ مِن مَّآءٖ دَافِقٖ"},{"number":7,"text":"يَخۡرُجُ مِنۢ بَيۡنِ ٱلصُّلۡبِ وَٱلتَّرَآئِبِ"},{"number":8,"text":"إِنَّهُۥ عَلَىٰ رَجۡعِهِۦ لَقَادِرٞ"},{"number":9,"text":"يَوۡمَ تُبۡلَى ٱلسَّرَآئِرُ"},{"number":10,"text":"فَمَا لَهُۥ مِن قُوَّةٖ وَلَا نَاصِرٖ"},{"number":11,"text":"وَٱلسَّمَآءِ ذَاتِ ٱلرَّجۡعِ"},{"number":12,"text":"وَٱلۡأَرۡضِ ذَاتِ ٱلصَّدۡعِ"},{"number":13,"text":"إِنَّهُۥ لَقَوۡلٞ فَصۡلٞ"},{"number":14,"text":"وَمَا هُوَ بِٱلۡهَزۡلِ"},{"number":15,"text":"إِنَّهُمۡ يَكِيدُونَ كَيۡدٗا"},{"number":16,"text":"وَأَكِيدُ كَيۡدٗا"},{"number":17,"text":"فَمَهِّلِ ٱلۡكَٰفِرِينَ أَمۡهِلۡهُمۡ رُوَيۡدَۢا"}]}